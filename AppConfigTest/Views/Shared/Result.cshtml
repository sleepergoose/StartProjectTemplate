@model AppConfigTest.Models.Result 

<div class="m-1 p-1">
    <table class="table table-bordered table-striped table-sm">
        <tr>
            <th>Controller:</th>
            <td>@Model.Controller</td>
        </tr>
        <tr>
            <th>Action:</th>
            <td>@Model.Action</td>
        </tr>
        @foreach (string key in Model.Data.Keys)
        {
<tr>
    <th>@key:</th>
    <td>@Model.Data[key]</td>
</tr>}
    </table>

    <a class="btn btn-primary"
       asp-action="CustomVariable"
       asp-controller="Home"
       asp-route-id="mon">

        This is outgoing URL
    </a>
    <br />
    <p>URL: @Url.Action("CustomVariable", "Home", new { id = 100 })</p>
</div>